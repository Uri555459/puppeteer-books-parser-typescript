{
	"name": "start-template-typescript",
	"version": "1.0.0",
	"main": "app.js",
	"license": "MIT",
	"scripts": {
		"preparser": "ts-node src/app.ts",
		"parser": "bash ./sendFile ./dataFull.json",
		"start:server": "ts-node src/server/index.ts",
		"send:file": "bash ./sendFile ./dataFull.json",
		"dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app.ts",
		"build": "tsc",
		"eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx src",
		"prettier": "prettier --write ./"
	},
	"devDependencies": {
		"@trivago/prettier-plugin-sort-imports": "^4.3.0",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/morgan": "^1.9.9",
		"@types/node": "^20.11.5",
		"@typescript-eslint/eslint-plugin": "^6.19.0",
		"@typescript-eslint/parser": "^6.19.0",
		"eslint": "^8.56.0",
		"nodemon": "^3.0.3",
		"prettier": "^3.2.4",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3"
	},
	"dependencies": {
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.0",
		"puppeteer": "^23.1.0"
	}
}
